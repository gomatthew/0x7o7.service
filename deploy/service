MySQL
docker run -d \
  --name mysql \
  -p 3306:3306 \
  -e MYSQL_ROOT_PASSWORD=makemoney \
  -e MYSQL_DATABASE=service_data_dev \
  -v /home/ubuntu/mysql_data:/var/lib/mysql \
  mysql:8.0
